<Window x:Class="DFU_Binding.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DFU_Binding"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1280" x:Name="MyWindow">

    <Grid>
        <Frame x:Name="PageViewer" Content="Frame" NavigationUIVisibility="Hidden"/>
        <Button Click="Next_Click" Content="Next" Margin="0,0,5,5" VerticalAlignment="Bottom" HorizontalAlignment="Right"/>
        <Button Click="Prev_Click" Content="Prev" Margin="5,0,0,5" VerticalAlignment="Bottom" HorizontalAlignment="Left"/>

    </Grid>
</Window>
